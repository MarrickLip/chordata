{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": [],
    "baseUrl": ".",
    "paths": {
      "~model/*" : ["../../model/*"],
      "~app/*" : ["./app/*"],
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
  ]
}
